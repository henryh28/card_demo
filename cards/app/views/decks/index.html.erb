<p><div class="heading">Deck list page</div></p>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
  </tr>

  <% @decks.each do |deck| %>
  <tr>
    <td><%= link_to deck.name, edit_deck_path(deck) %></td>
    <td><%= deck.deck_type %></td>
  </tr>
  <% end %>
</table>

<br>
<%= link_to "Home", root_path %>
